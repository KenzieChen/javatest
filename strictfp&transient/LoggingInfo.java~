import java.io.Serializable;
class LoggingInfo implements Serializble{
	private Date loggingDate = new Date();
	private String uid;
	private transient String pwd;
	public LogingInfo(String uid, String pwd){
		this.uid = uid;
		this.pwd = pwd;
	}
	public String toString(){
		String password =null;
		if(pwd == null){
			password = "Not Set";
		}else{
			password = pwd;
		}
		return "id:"+uid+"/n password:"+password+"/n loggingDate:"+logingDate;
	}
}
public class TransientTest{
	public static void main(String[] args){
		LoggingInfo info = new LoggingInfo("123","123456");
		try{
			File file = new File("Logging.out");
			if(!file.exists()){
				file.createNewFile();
			}
		}catch(IOException ioe){
			ioe.printStackTrace()
		}
		ObjectOutputStream oos = null;
		ObjectInputStream ois = null;
		try{
			oos = new ObjectOutputStream(new FileOutputStream(file));
			oos.writeObject(oos);
			ois = new ObjectInputStream(new FileInputStream(file));
			LoggingInfo info = (LoggingInfo)ois.readObject();
			System.out.println(info.toString());
		}catch(IOException ioe2){
			ioe2.printStackTrace();	
		}finally{
			
			oos.close();
			ois.close();
		}

		

	}
}
